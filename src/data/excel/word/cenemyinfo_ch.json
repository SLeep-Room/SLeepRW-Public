    "Data": {
        "2200064": {
            "id": 2200064,
            "text": "瞬移到虚空躲避伤害。"
        },
        "2200065": {
            "id": 2200065,
            "text": "汲取"
        },
        "2200066": {
            "id": 2200066,
            "text": "德亚丝能吸取我方的混沌能量并永久提升自己的攻击。"
        },
        "2200067": {
            "id": 2200067,
            "text": "黑暗侵袭"
        },
        "2200068": {
            "id": 2200068,
            "text": "德亚丝会集中能量对当前生命最低的人偶造成高额魔法伤害。"
        },
        "2200069": {
            "id": 2200069,
            "text": "呼唤同伴"
        },
        "2200070": {
            "id": 2200070,
            "text": "雷龙会召唤魔飞龙协助战斗。"
        },
        "2200071": {
            "id": 2200071,
            "text": "唤雷"
        },
        "2200072": {
            "id": 2200072,
            "text": "雷龙会让最前排的人偶陷入麻痹状态。"
        },
        "2200073": {
            "id": 2200073,
            "text": "魔物袭来"
        },
        "2200074": {
            "id": 2200074,
            "text": "费拉古德会依次召唤飞翼魔狼、魔飞蛇、杀人鲸协助战斗。"
        },
        "2200075": {
            "id": 2200075,
            "text": "锁定目标"
        },
        "2200076": {
            "id": 2200076,
            "text": "执行双生会优先攻击最后排的人偶。"
        },
        "2200077": {
            "id": 2200077,
            "text": "圣域·终结"
        },
        "2200078": {
            "id": 2200078,
            "text": "执行双生生命较低时会使用终结技能，让人偶直接陷入无法继续战斗的状态。"
        },
        "2200079": {
            "id": 2200079,
            "text": "呼唤魔物"
        },
        "2200080": {
            "id": 2200080,
            "text": "索拉迪乌斯会频繁召唤幼体魔物协助战斗。"
        },
        "2200081": {
            "id": 2200081,
            "text": "魔力侵蚀"
        },
        "2200082": {
            "id": 2200082,
            "text": "索拉迪乌斯会对前排人偶造成流血效果，使其在一段时间内无法回复生命。"
        },
        "2200083": {
            "id": 2200083,
            "text": "祸乱之眼"
        },
        "2200084": {
            "id": 2200084,
            "text": "每隔一段时间索拉迪乌斯会降低我方人偶所造成的伤害。"
        },
        "2200085": {
            "id": 2200085,
            "text": "眼棱"
        },
        "2200086": {
            "id": 2200086,
            "text": "索拉迪乌斯生命值较低时，会从眼部发射出光线，持续攻击我方人偶。"
        },
        "2200087": {
            "id": 2200087,
            "text": "相位移动"
        },
        "2200088": {
            "id": 2200088,
            "text": "艾迪恩会连续闪烁3次，每次闪烁都会对我方人偶造成伤害。艾迪恩每次闪烁前都可以被控制技能打断。"
        },
        "2200089": {
            "id": 2200089,
            "text": "分身"
        },
        "2200090": {
            "id": 2200090,
            "text": "艾迪恩会变成三个幼体，分别对前排人偶造成伤害。"
        },
        "2200091": {
            "id": 2200091,
            "text": "入梦"
        },
        "2200092": {
            "id": 2200092,
            "text": "艾迪恩进入入梦状态，大幅提升自身的伤害。"
        },
        "2200093": {
            "id": 2200093,
            "text": "虚张声势"
        },
        "2200094": {
            "id": 2200094,
            "text": "生命值较低时会躲在梦魇护盾后为下一步进化做准备，若在准备期间受到伤害，进化会失败。"
        },
        "2200095": {
            "id": 2200095,
            "text": "阴暗聚集"
        },
        "2200096": {
            "id": 2200096,
            "text": "影之惧进化成恐惧之源后战斗力会大幅提升。"
        },
        "2200097": {
            "id": 2200097,
            "text": "虚张声势"
        },
        "2200098": {
            "id": 2200098,
            "text": "生命值较低时会躲在梦魇护盾后为下一步进化做准备，若在准备期间受到伤害，进化会失败。"
        },
        "2200099": {
            "id": 2200099,
            "text": "阴影"
        },
        "2200100": {
            "id": 2200100,
            "text": "恐惧会释放阴影包围我方人偶，它会伺机隐藏到阴影之中，躲避所有伤害。"
        },
        "2200101": {
            "id": 2200101,
            "text": "阴暗聚集"
        },
        "2200102": {
            "id": 2200102,
            "text": "恐惧之源进化成深渊的绝望后战斗力会大幅提升。"
        },
        "2200103": {
            "id": 2200103,
            "text": "阴影"
        },
        "2200104": {
            "id": 2200104,
            "text": "恐惧会释放阴影包围我方人偶，它会伺机隐藏到阴影之中，躲避所有伤害。"
        },
        "2200105": {
            "id": 2200105,
            "text": "梦魇护盾"
        },
        "2200106": {
            "id": 2200106,
            "text": "深渊的绝望会释放护盾保护自己，并趁机躲在护盾后面准备爆发，在准备期间护盾可以被打破。"
        },
        "2200107": {
            "id": 2200107,
            "text": "阴影爆发"
        },
        "2200108": {
            "id": 2200108,
            "text": "将阴暗的能量全部释放出来，造成高额的伤害。"
        },
        "2200109": {
            "id": 2200109,
            "text": "逐级试炼"
        },
        "2200110": {
            "id": 2200110,
            "text": "索卢克每损失一定的生命值，所造成的伤害就会进一步增加。"
        },
        "2200111": {
            "id": 2200111,
            "text": "失去耐心"
        },
        "2200112": {
            "id": 2200112,
            "text": "战斗时间若拖延过久，索卢克会终结我方全体人偶。"
        },
        "2200113": {
            "id": 2200113,
            "text": "魔力切换"
        },
        "2200114": {
            "id": 2200114,
            "text": "索卢克可以切换攻击形态，造成物理或者魔法伤害。"
        },
        "2200115": {
            "id": 2200115,
            "text": "恃强凌弱"
        },
        "2200116": {
            "id": 2200116,
            "text": "如果我方在场的人偶数量少于3，那么索卢克会连续释放锤击。"
        },
        "2200117": {
            "id": 2200117,
            "text": "忍耐就是胜利"
        },
        "2200118": {
            "id": 2200118,
            "text": "战斗时长达到一定值时，我方人偶会直接胜利。"
        },
        "2200119": {
            "id": 2200119,
            "text": "魔力切换"
        },
        "2200120": {
            "id": 2200120,
            "text": "索卢克可以切换攻击形态，造成物理或者魔法伤害。"
        },
        "2200121": {
